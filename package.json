{
    "name": "typescript-console-app-template",
    "version": "0.0.0",
    "author": {
        "name": "Chris O'Brien"
    },
    "license": "MIT",
    "scripts": {
        "prettier:ci": "npx prettier . --check",
        "prettier:fix": "npx prettier . --write",
        "lint:ci": "npx eslint . --max-warnings=0",
        "lint:fix": "npx eslint . --fix",
        "audit:ci": "npm audit --audit-level=none",
        "audit:fix": "npm audit fix --force",
        "build:ci": "npm run versions && npm outdated && npm run audit:ci && npm ci && npm audit && npm run prettier:ci && npm run lint:ci",
        "build:local": "npm run versions && npm install && npm update --save && npm run prettier:fix && npm run lint:fix",
        "versions": "node -v && npm -v",
        "start": "npm run build:local && npm run run",
        "precommit": "npm run build:local && npm run build:ci && npm run run",
        "run": "set NODE_ENV=example && ts-node src/program.ts"
    },
    "dependencies": {
        "dotenv": "^16.3.1",
        "ts-command-line-args": "^2.5.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "eslint": "^8.47.0",
        "npm": "^9.8.1",
        "prettier": "^3.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
